<Defs>
  <!-- Tail-->
  <RecipeDef ParentName="SurgeryInstallBodyPartNaturalBase">
    <defName>InstallPathTailTail</defName>
    <label>install tail</label>
    <description>Installs tail.</description>
    <workerClass>Recipe_InstallNaturalBodyPart</workerClass>
    <jobString>Installing tail.</jobString>
    <workAmount>1200</workAmount>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>Medicine</li>
          </categories>
        </filter>
        <count>1</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>PathTail</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <categories>
        <li>Medicine</li>
      </categories>
      <thingDefs>
        <li>PathTail</li>
      </thingDefs>
    </fixedIngredientFilter>
    <appliedOnFixedBodyParts>
      <li>PathTail</li>
    </appliedOnFixedBodyParts>
  </RecipeDef>

